{
  "name": "vue-bootstrap-template",
  "version": "1.0.0",
  "description": "Template for Vue.js and Bootstrap application",
  "private": true,
  "scripts": {
    "lint": "npx eslint ./src ./test",
    "dev": "npx webpack --mode development",
    "watch": "npx webpack --mode development --watch",
    "test": "npx mocha",
    "coverage": "npx nyc mocha && nyc report --reporter=text-lcov > coverage.lcov",
    "prod": "npm run lint && npm run test && npx webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timmson/vue-bootstrap-template.git"
  },
  "author": "timmnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timmson/vue-bootstrap-template/issues"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "jquery": "3.4.1",
    "popper.js": "1.15.0",
    "vue": "2.6.10"
  },
  "devDependencies": {
    "autoprefixer": "9.6.1",
    "chai": "^4.2.0",
    "css-loader": "3.2.0",
    "eslint": "6.2.2",
    "mini-css-extract-plugin": "0.8.0",
    "mocha": "^6.2.0",
    "node-sass": "4.12.0",
    "nyc": "14.1.1",
    "postcss-loader": "3.0.0",
    "precss": "4.0.0",
    "sass-loader": "7.2.0",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.6"
  }
}
