{
  "name": "vue-bootstrap-template",
  "version": "1.1.0",
  "description": "Template for Vue.js and Bootstrap application",
  "private": true,
  "scripts": {
    "lint": "npx eslint ./src ./test --fix",
    "dev": "npx webpack --mode development",
    "watch": "npx webpack --mode development --watch",
    "test": "npx mocha",
    "coverage": "npx nyc mocha && nyc report --reporter=text-lcov > coverage.lcov",
    "prod": "npm run lint && npm run test && npx webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timmson/vue-bootstrap-template.git"
  },
  "author": "timmnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timmson/vue-bootstrap-template/issues"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.0",
    "popper.js": "^1.16.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/preset-env": "7.9.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "autoprefixer": "^9.7.6",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.2",
    "node-sass": "^4.14.1",
    "nyc": "^15.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
