{
  "name": "vue-bootstrap-template",
  "version": "1.2.0",
  "description": "Template for Vue.js and Bootstrap application",
  "private": true,
  "scripts": {
    "lint": "npx eslint ./src ./test --fix",
    "dev": "npx webpack --mode development",
    "watch": "npx webpack --mode development --watch",
    "test": "npx jest",
    "coverage": "npx jest --collectCoverage",
    "prod": "npm run lint && npm run test && npx webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timmson/vue-bootstrap-template.git"
  },
  "author": "timmnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timmson/vue-bootstrap-template/issues"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.7",
    "@types/jest": "26.0.20",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.14.0",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^6.0.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^5.3.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
